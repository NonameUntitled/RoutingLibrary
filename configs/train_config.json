{
  "experiment_name": "ppo_train",
  "type": "conveyor",
  "path_memory": {
    "type": "shared_path_memory"
  },
  "learn_trigger_bag_count": 30,
  "topology": {
    "type": "oriented",
    "sources": {
      "0": {
        "upstream_conv": 0
      }
    },
    "sinks": [
      0,
      1
    ],
    "diverters": {
      "0": {
        "conveyor": 0,
        "pos": 20,
        "upstream_conv": 1
      },
      "1": {
        "conveyor": 1,
        "pos": 10,
        "upstream_conv": 2
      }
    },
    "conveyors": {
      "0": {
        "length": 40,
        "upstream": {
          "type": "sink",
          "id": 0
        }
      },
      "1": {
        "length": 20,
        "upstream": {
          "type": "sink",
          "id": 1
        }
      },
      "2": {
        "length": 10,
        "upstream": {
          "type": "conveyor",
          "id": 0,
          "pos": 10
        }
      }
    }
  },
  "topology_2": {
    "type": "monte_carlo",
    "seed": 42,
    "sources_cnt": 10,
    "sinks_cnt": 10,
    "vertices_cnt": 10,
    "edges_cnt": 20,
    "generate_cycles": true,
    "min_edge_length": 10,
    "max_edge_length": 100,
    "length_distribution": "uniform"
  },
  "agent_1": {
    "type": "random",
    "neighbors_prefix": "neighbors_node_ids",
    "output_prefix": "next_nodes"
  },
  "agent_2": {
    "type": "dqn",
    "current_node_idx_prefix": "current_node_idx",
    "destination_node_idx_prefix": "destination_node_idx",
    "neighbors_node_ids_prefix": "neighbors_node_ids",
    "bag_ids_prefix": "bag_ids",
    "output_prefix": "next_nodes",
    "q_network": {
      "type": "tower_q_network",
      "embedder": {
        "shared": true,
        "type": "learnable"
      },
      "ff_net": {
        "type": "tower",
        "input_dim": 512,
        "hidden_dims": [
          256,
          256
        ],
        "output_dim": 1
      }
    },
    "path_memory": {
      "type": "shared_path_memory"
    },
    "optimizer": {
      "type": "basic",
      "optimizer": {
        "type": "adamw",
        "lr": 3e-4,
        "eps": 1e-8
      },
      "clip_grad_threshold": 5.0
    }
  },
  "agent": {
    "type": "ppo",
    "current_node_idx_prefix": "current_node_idx",
    "destination_node_idx_prefix": "destination_node_idx",
    "neighbors_node_ids_prefix": "neighbors_node_ids",
    "bag_ids_prefix": "bag_ids",
    "output_prefix": "next_nodes",
    "actor": {
      "type": "tower_actor",
      "embedder": {
        "shared": true,
        "type": "learnable"
      },
      "ff_net": {
        "type": "tower",
        "input_dim": 256,
        "hidden_dims": [
          256,
          256
        ],
        "output_dim": 256
      }
    },
    "critic": {
      "type": "tower_critic",
      "embedder": {
        "shared": true,
        "type": "learnable"
      },
      "ff_net": {
        "type": "tower",
        "input_dim": 256,
        "hidden_dims": [
          64,
          64
        ],
        "output_dim": 1
      }
    },
    "path_memory": {
      "type": "shared_path_memory"
    },
    "optimizer": {
      "type": "basic",
      "optimizer": {
        "type": "adamw",
        "lr": 3e-3,
        "eps": 1e-8
      },
      "clip_grad_threshold": 5.0
    }
  },
  "pretrain": {
    "type": "dijkstra",
    "stable_topology": true,
    "steps_num": 5000,
    "current_node_prefix": "current_node",
    "neighbors_nodes_prefix": 123,
    "target_node_prefix": 123,
    "next_node_prefix": 123,
    "optimizer": {
      "type": "adam"
    }
  },
  "train": {
    "multi_agent": true,
    "type": "scenario",
    "scenario_1": {
      "type": "from_file"
    },
    "scenario_2": {
      "type": "sequence"
    },
    "scenario_3": {
      "type": "random",
      "here_we_need_some_variabes": true
    }
  },
  "shared_embedder": {
    "shared": true,
    "type": "learnable",
    "embedding_dim": 256,
    "vocabulary_size": 40
  }
}
